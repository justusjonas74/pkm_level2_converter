	<!--Verarbeitungsdaten-->
	<xs:complexType name="XML-Strategie-Part">
		<xs:sequence>
			<xs:element name="start" type="XML-Ref-11"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XML-Schritt-Item">
		<xs:complexContent>
			<xs:extension base="XML-Name-Item">
				<xs:choice>
					<xs:element name="ausgang" type="XML-Ausgangsschritt-Part"/>
